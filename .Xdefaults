!Xcursor.theme:    Vanilla-DMZ-AA

!urxvt color scheme:

URxvt*background: #2B2B2B
URxvt*foreground: #DEDEDE

URxvt*colorUL: #86a2b0

! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC

Xft.dip: 96
Xft.antialias: true
Xft:rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight

!urxvt fonts: 

!URxvt*font: xft:Monospace:pixelsize=12
!URxvt*boldFont: xft:Monospace:bold:pixelsize=12
URxvt*font: xft:Ubuntu Mono:pixelsize=14
URxvt*boldFont: xft:Ubuntu Mono:bold:pixelsize=14

!urxvt extensions:
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.Control-t: perl:tabbedex:new_tab
URxvt.iso14755: False
URxvt.perl-ext-common:  default,matcher,tabbedex,clipboard

!enabling clickable links:

URxvt.urlLauncher:      /usr/bin/firefox
URxvt.matcher.button:   1 

!urxvt scrolling options and cursor style:

URxvt*saveLines: 12000
URxvt*scrollstyle:plain
URxvt*scrollBar: false
URxvt*cursorBlink: true
!URxvt*cursorUnderline: true

! urxvt tabs settings:

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 2
URxvt.tabbed.tab-bg: 8
URxvt.tabbed.autohide:  yes
URxvt.tabbed.new-button:  no
URxvt.tabbed.title: no


URxvt.depth: 32
URxvt.background: rgba:1111/1111/1111/dddd
